{
  "name": "ultra-math-preview",
  "displayName": "Ultra Math Preview",
  "description": "useful tools for math input",
  "keywords": [
    "math",
    "math preview",
    "markdown",
    "latex"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yfzhao20/vscode-math-preview"
  },
  "bugs": {
    "url": "https://github.com/yfzhao20/vscode-math-preview/issues"
  },
  "version": "0.0.5",
  "publisher": "yfzhao20",
  "preview": true,
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onLanguage:latex"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "umath.preview.closeAllPreview",
        "title": "Ultra Math Preview: Close All Preview"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "umath.preview.closeAllPreview",
          "group": "navigation",
          "when": "editorFocus"
        }
      ]
    }
  },
  "extensionDependencies": [
    "draivin.hscopes"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3"
  },
  "dependencies": {
    "mathjax": "^3.2.0"
  }
}
